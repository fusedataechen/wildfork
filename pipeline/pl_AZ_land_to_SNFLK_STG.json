									"name": "survey_prople_email",
									"type": "String"
								},
								"sink": {
									"name": "PERSON_RESPONSE_EMAIL",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "survey_prople_created_at",
									"type": "Int64"
								},
								"sink": {
									"name": "PERSON_RESPONSE_CREATED_AT",
									"type": "Int64"
								}
